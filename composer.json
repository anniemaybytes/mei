{
  "name": "animebytes/mei",
  "description": "Mei Image Server",
  "type": "project",
  "license": "Unlicense",
  "scripts": {
    "test": [
      "@phplint",
      "@phpcs",
      "@phpstan",
      "@phpunit"
    ],
    "phpstan": "phpstan analyze --ansi --no-progress --memory-limit=-1",
    "phpunit": "phpunit --colors=always",
    "phplint": "phplint --ansi -n",
    "phpcs": "phpcs -n --cache=.phpcs-cache",
    "phpcbf": "phpcbf",
    "phinx": "phinx"
  },
  "require": {
    "php": "~8.3",
    "slim/slim": "4.14.0",
    "tracy/tracy": "2.10.7",
    "paragonie/random-lib": "2.0.3",
    "php-di/php-di": "7.0.7",
    "slim/psr7": "1.7.0",
    "slim/http": "1.4.0",
    "slim/http-cache": "1.2.0",
    "robmorgan/phinx": "0.16.2",
    "jdorn/sql-formatter": "dev-master#7ef9b85961956aa572413693e1194b60f50ab9ab",
    "ext-xml": "*",
    "ext-json": "*",
    "ext-gd": "*",
    "ext-pdo": "*",
    "ext-imagick": "*",
    "ext-zlib": "*",
    "ext-date": "*",
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-zend-opcache": "*",
    "ext-fileinfo": "*"
  },
  "autoload": {
    "psr-4": {
      "Mei\\": "src/Mei/",
      "Singleton\\": "src/Singleton/",
      "RunTracy\\": "src/RunTracy/"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpstan/extension-installer": "1.4.1",
    "phpstan/phpstan": "1.11.9",
    "phpstan/phpstan-deprecation-rules": "1.2.0",
    "phpstan/phpstan-phpunit": "1.4.0",
    "overtrue/phplint": "9.4.1",
    "squizlabs/php_codesniffer": "3.10.2",
    "mikey179/vfsstream": "1.6.11",
    "dg/bypass-finals": "1.8.0",
    "phpunit/phpunit": "11.3.0"
  },
  "autoload-dev": {
    "psr-0": {
      "Mei": "tests"
    }
  },
  "config": {
    "platform-check": true,
    "platform": {
      "php": "8.3.0"
    },
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
