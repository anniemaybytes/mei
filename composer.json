{
  "name": "animebytes/mei",
  "description": "Mei Image Server",
  "type": "project",
  "license": "Unlicense",
  "scripts": {
    "test": [
      "@phplint",
      "@phpcs",
      "@phpstan",
      "@phpunit"
    ],
    "phpstan": "phpstan analyze --ansi --no-progress --memory-limit=-1",
    "phpunit": "phpunit --colors=always",
    "phplint": "phplint --ansi -n",
    "phpcs": "phpcs -n --cache=.phpcs-cache",
    "phpcbf": "phpcbf",
    "phpmig": "phpmig"
  },
  "require": {
    "php": "~8.1",
    "slim/slim": "4.10.0",
    "tracy/tracy": "2.9.3",
    "paragonie/random-lib": "2.0.1",
    "pimple/pimple": "3.5.0",
    "php-di/php-di": "6.4.0",
    "slim/psr7": "1.5",
    "slim/http": "1.2.0",
    "davedevelopment/phpmig": "1.7.0",
    "doctrine/annotations": "1.13.2",
    "petrknap/php-singleton": "1.0.0",
    "jdorn/sql-formatter": "dev-master#7ef9b85961956aa572413693e1194b60f50ab9ab",
    "ext-xml": "*",
    "ext-json": "*",
    "ext-gd": "*",
    "ext-pdo": "*",
    "ext-imagick": "*",
    "ext-zlib": "*",
    "ext-date": "*",
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-zend-opcache": "*"
  },
  "autoload": {
    "psr-4": {
      "Mei\\": "src/Mei/",
      "RunTracy\\": "src/RunTracy/"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpstan/extension-installer": "1.1.0",
    "phpstan/phpstan": "1.7.15",
    "phpstan/phpstan-deprecation-rules": "1.0.0",
    "phpstan/phpstan-phpunit": "1.1.1",
    "overtrue/phplint": "4.2.1",
    "squizlabs/php_codesniffer": "3.7.1",
    "mikey179/vfsstream": "1.6.10",
    "dg/bypass-finals": "1.3.1",
    "phpunit/phpunit": "9.5.21"
  },
  "autoload-dev": {
    "psr-0": {
      "Mei": "tests"
    }
  },
  "config": {
    "platform-check": true,
    "platform": {
      "php": "8.1.0"
    },
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
