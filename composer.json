{
    "name": "animebytes/mei-image-server",
    "description": "Tentacles Image Server",
    "authors": [
        {
            "name": "AnimeBytes",
            "email": "staff@animebytes.tv"
        }
    ],
    "scripts": {
        "test": [
            "@phplint",
            "@phpcs",
            "@phpstan",
            "@phpspec"
        ],
        "phpstan": [
            "phpstan analyze --ansi --memory-limit=-1"
        ],
        "phpspec": [
            "phpspec run --ansi"
        ],
        "phplint": [
            "phplint --ansi -n"
        ],
        "phpcs": [
            "phpcs -n --cache=.phpcs-cache"
        ],
        "phpcbf": [
            "phpcbf"
        ],
        "phinx": [
            "phinx"
        ]
    },
    "require": {
        "php": "~7.3",
        "slim/slim": "4.4.0",
        "tracy/tracy": "2.7.3",
        "paragonie/random-lib": "2.0.1",
        "robmorgan/phinx": "0.9.2",
        "php-di/php-di": "6.0.11",
        "slim/psr7": "1.0",
        "slim/http": "1.0",
        "doctrine/annotations": "1.8.0",
        "petrknap/php-singleton": "1.0.0",
        "jdorn/sql-formatter": "dev-master#7ef9b85961956aa572413693e1194b60f50ab9ab",
        "ext-xml": "*",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-pdo": "*",
        "ext-imagick": "*",
        "ext-zlib": "*",
        "ext-date": "*",
        "ext-openssl": "*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Mei\\": "src/Mei/",
            "RunTracy\\": "src/RunTracy/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpstan/phpstan": "0.12.14",
        "overtrue/phplint": "1.2.1",
        "phpspec/phpspec": "6.1.1",
        "squizlabs/php_codesniffer": "3.5.4"
    }
}
