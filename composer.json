{
  "name": "animebytes/mei",
  "description": "Mei Image Server",
  "type": "project",
  "license": "Unlicense",
  "scripts": {
    "test": [
      "@phplint",
      "@phpcs",
      "@phpstan",
      "@phpunit"
    ],
    "phpstan": [
      "phpstan analyze --ansi --memory-limit=-1"
    ],
    "phpunit": [
      "phpunit --colors=always"
    ],
    "phplint": [
      "phplint --ansi -n"
    ],
    "phpcs": [
      "phpcs -n --cache=.phpcs-cache"
    ],
    "phpcbf": [
      "phpcbf"
    ],
    "phpmig": [
      "phpmig"
    ]
  },
  "require": {
    "php": "~8.0",
    "slim/slim": "4.8.1",
    "tracy/tracy": "2.8.5",
    "paragonie/random-lib": "2.0.1",
    "pimple/pimple": "3.4.0",
    "php-di/php-di": "6.3.4",
    "slim/psr7": "1.4",
    "slim/http": "1.2.0",
    "davedevelopment/phpmig": "1.7.0",
    "doctrine/annotations": "1.13.1",
    "petrknap/php-singleton": "1.0.0",
    "jdorn/sql-formatter": "dev-master#7ef9b85961956aa572413693e1194b60f50ab9ab",
    "ext-xml": "*",
    "ext-json": "*",
    "ext-gd": "*",
    "ext-pdo": "*",
    "ext-imagick": "*",
    "ext-zlib": "*",
    "ext-date": "*",
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-zend-opcache": "*"
  },
  "autoload": {
    "psr-4": {
      "Mei\\": "src/Mei/",
      "RunTracy\\": "src/RunTracy/"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpstan/phpstan": "0.12.90",
    "phpstan/phpstan-deprecation-rules": "0.12.6",
    "overtrue/phplint": "3.0.2",
    "squizlabs/php_codesniffer": "3.6.0",
    "mikey179/vfsstream": "1.6.8",
    "dg/bypass-finals": "1.3.1",
    "phpunit/phpunit": "9.5.6"
  },
  "config": {
    "platform-check": true,
    "platform": {
      "php": "8.0.0"
    }
  }
}
