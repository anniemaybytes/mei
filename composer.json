{
    "name": "animebytes/mei-image-server",
    "description": "Tentacles Image Server",
    "authors": [
        {
            "name": "AnimeBytes",
            "email": "staff@animebytes.tv"
        }
    ],
    "require": {
        "php": "~7.3",
        "slim/slim": "^4.4.0",
        "tracy/tracy": "^2.7.2",
        "paragonie/random-lib": "^2.0.1",
        "php-di/php-di": "^6.0.11",
        "slim/psr7": "^1.0",
        "slim/http": "^1.0",
        "doctrine/annotations": "^1.8.0",
        "petrknap/php-singleton": "1.0.0",
        "ext-xml": "*",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-imagick": "*",
        "ext-zlib": "*",
        "ext-date": "*",
        "ext-openssl": "*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Mei\\": "src/Mei/",
            "RunTracy\\": "src/RunTracy/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpstan/phpstan": "^0.12.7"
    }
}
